<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ebreadshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.ebreadshop.ViewFoodActivity"></activity>
        <activity android:name="com.example.ebreadshop.CRUDFoodActivity" />
        <activity android:name="com.example.ebreadshop.FoodListActivity" />
        <activity android:name="com.example.ebreadshop.AddFoodItemActivity" />
        <activity android:name="com.example.ebreadshop.MenuManagementActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>