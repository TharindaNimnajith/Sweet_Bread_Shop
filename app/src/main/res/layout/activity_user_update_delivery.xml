<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_google_signin_btn_text_dark_default"
    tools:context=".myapplication.deliveryHome">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/ConstraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/DhomeDelete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/delete"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DhomeSubmit" />

        <Button
            android:id="@+id/DhomeSubmit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/update"
            app:layout_constraintBottom_toTopOf="@+id/DhomeDelete"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:background="@color/common_google_signin_btn_text_dark_default"
            android:text="@string/ddeerer"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeCurrentdate"
            app:layout_constraintEnd_toStartOf="@+id/DhomeCurrentdate"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeCurrentdate" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:background="@color/common_google_signin_btn_text_dark_default"
            android:text="@string/OrderID"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/dhomeUserID"
            app:layout_constraintEnd_toStartOf="@+id/dhomeUserID"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dhomeUserID" />

        <EditText
            android:id="@+id/dhomeUserID"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/DZipcode"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/dhomeOrderID"
            tools:targetApi="o" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="47dp"
            android:background="@color/common_google_signin_btn_text_dark_default"
            android:text="@string/OrderID"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/dhomeOrderID"
            app:layout_constraintEnd_toStartOf="@+id/dhomeOrderID"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dhomeOrderID"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="47dp"
            android:background="@color/common_google_signin_btn_text_dark_default"
            android:text="@string/OrderID"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/dhomeOrderID"
            app:layout_constraintEnd_toStartOf="@+id/dhomeOrderID"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/dhomeOrderID"
            app:layout_constraintVertical_bias="0.0" />

        <EditText
            android:id="@+id/DhomeAddress"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="16dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/DZipcode"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dimageView4"
            app:layout_constraintTop_toBottomOf="@+id/city"
            app:layout_constraintVertical_bias="0.0"
            tools:targetApi="o" />

        <EditText
            android:id="@+id/dhomeOrderID"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/DZipcode"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintTop_toTopOf="parent"
            tools:targetApi="o" />

        <EditText
            android:id="@+id/DhomeName"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/name"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/pro"
            app:layout_constraintTop_toBottomOf="@+id/DhomeAddress2"
            tools:targetApi="o" />

        <EditText
            android:id="@+id/DhomeTp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/Dcontract_number"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dimageView2"
            app:layout_constraintTop_toBottomOf="@+id/DhomeName"
            app:layout_constraintVertical_bias="1.0"
            tools:targetApi="o" />

        <EditText
            android:id="@+id/DhomeTP2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="8dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/Dcontract_number"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dimageView3"
            app:layout_constraintTop_toBottomOf="@+id/DhomeTp"
            app:layout_constraintVertical_bias="0.0"
            tools:targetApi="o" />

        <EditText
            android:id="@+id/DhomeAddress2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/DState"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dimageView4"
            app:layout_constraintTop_toBottomOf="@+id/DhomeAddress"
            tools:targetApi="o" />

        <EditText
            android:id="@+id/DhomeTime"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:autofillHints="no"
            android:background="@drawable/rr"
            android:ems="10"
            android:hint="@string/Dtime"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dimageView7"
            app:layout_constraintTop_toBottomOf="@+id/DhomeDate"
            tools:targetApi="o" />

        <ImageView
            android:id="@+id/dimageView4"
            android:layout_width="wrap_content"
            android:layout_height="71dp"
            android:layout_marginStart="36dp"
            android:contentDescription="@string/Daddress"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeAddress2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeAddress"
            app:srcCompat="@drawable/location" />

        <ImageView
            android:id="@+id/pro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:contentDescription="@string/todo"
            android:src="@drawable/name"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeName"
            app:layout_constraintEnd_toStartOf="@+id/DhomeName"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeName" />

        <ImageView
            android:id="@+id/dimageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginStart="34dp"
            android:contentDescription="@string/Dcontract_number"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeTp"
            app:layout_constraintEnd_toStartOf="@+id/DhomeTp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeTp"
            app:srcCompat="@drawable/c" />

        <ImageView
            android:id="@+id/dimageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginStart="34dp"
            android:contentDescription="@string/Dcontract_number"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeTP2"
            app:layout_constraintEnd_toStartOf="@+id/DhomeTP2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeTP2"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/c" />

        <ImageView
            android:id="@+id/dimageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:contentDescription="@string/Dcontract_number"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeDate"
            app:layout_constraintEnd_toStartOf="@+id/DhomeDate"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeDate"
            app:srcCompat="@drawable/ddd" />

        <ImageView
            android:id="@+id/dimageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="34dp"
            android:contentDescription="@string/Dcontract_number"
            app:layout_constraintBottom_toBottomOf="@+id/DhomeTime"
            app:layout_constraintEnd_toStartOf="@+id/DhomeTime"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/DhomeTime"
            app:srcCompat="@drawable/tim" />

        <Spinner
            android:id="@+id/city"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="20dp"
            android:dropDownWidth="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/DhomeCurrentdate"
            app:layout_constraintVertical_bias="0.064" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DPRICE"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/Dhomeprice"
            app:layout_constraintEnd_toStartOf="@+id/city"
            app:layout_constraintHorizontal_bias="0.774"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/Dhomeprice" />

        <TextView
            android:id="@+id/Dhomeprice"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="8dp"
            android:textAlignment="center"
            android:textColor="@color/design_default_color_primary"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/DhomeSubmit"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/DhomeTime" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:background="@color/common_google_signin_btn_text_dark_default"
            android:text="@string/udatese"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="@+id/city"
            app:layout_constraintEnd_toStartOf="@+id/city"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/city"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/DhomeDate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="3dp"
            android:background="@drawable/rr"
            android:text="@string/Ddate"
            android:textAlignment="center"
            android:textColor="@color/cardview_dark_background"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dimageView5"
            app:layout_constraintTop_toBottomOf="@+id/DhomeTP2"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/DhomeCurrentdate"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView10"
            app:layout_constraintTop_toBottomOf="@+id/dhomeUserID"
            app:layout_constraintVertical_bias="0.013" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>